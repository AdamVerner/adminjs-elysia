{
  "name": "adminjs-elysia",
  "version": "0.1.3",
  "description": "Unofficial AdminJS plugin which integrates it with Elysia",
  "author": "Adam Verner <averner@averner.cz>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": " tsc --project tsconfig.json",
    "prepublishOnly": "bun run build",
    "example:minimal": "bun run example/minimal.ts",
    "example:auth": "bun run --watch example/auth.ts",
    "example:drizzle": "bun run example/drizzle.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "admin",
    "adminjs",
    "admin-panel",
    "elysia",
    "bun"
  ],
  "license": "MIT",
  "homepage": "https://github.com/AdamVerner/adminjs-elysia#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdamVerner/adminjs-elysia.git"
  },
  "bugs": "https://github.com/AdamVerner/adminjs-elysia/issues",
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/pg": "^8.11.6",
    "adminjs-drizzle": "git+https://github.com/makuko/adminjs-drizzle.git",
    "bun-plugin-dts": "^0.2.3",
    "pg": "^8.12.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "adminjs": "^7.8.8",
    "elysia": "^1.1.5",
    "@elysiajs/jwt": "^1.1.0",
    "node-mocks-http": "^1.15.1"
  },
  "dependencies": {  }
